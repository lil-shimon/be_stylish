<h1>Users#edit</h1>
<p>Find me in app/views/users/edit.html.erb</p>
<%= form_for @user do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :profile %>
  <%= f.text_area :profile %>

  <%= f.label :profile_image %>
  <%= f.attachment_field :profile_image %>

  <%= f.submit %>
<% end %>
  
